<local:PageBase x:Class="GTA6Game.Pages.LoadingMission"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:gif="http://wpfanimatedgif.codeplex.com"
      xmlns:local="clr-namespace:GTA6Game.Pages" xmlns:usercontrols="clr-namespace:GTA6Game.UserControls"
                mc:Ignorable="d" 
      d:DesignHeight="710" d:DesignWidth="1200"
      Title="LoadingMission" Background="Black" x:Name="Windows" Loaded="Windows_Loaded">

    <Canvas>
        <Image x:Name="ImgBackground" Height="604" Source="/Assets/Backgrounds/Bg1.png" Canvas.Top="71" Width="1116" Canvas.Left="1230"/>
        <Image x:Name="ImgScprite" Height="551" Canvas.Left="1749" Source="/Assets/Backgrounds/Sp1.png" VerticalAlignment="Bottom" Canvas.Top="173" Width="500"/>
        <Image RenderOptions.BitmapScalingMode="HighQuality" gif:ImageBehavior.AnimatedSource="/Assets/LoadingSpinner.gif" Stretch="Fill" Height="34" Width="34" Canvas.Left="1139" Canvas.Top="665"/>
        <usercontrols:UserIcon Style="{StaticResource Fake}" ImageSource="/Assets/GtaLogo1.png" Height="85" Width="129" Canvas.Left="19" Canvas.Top="608"/>
